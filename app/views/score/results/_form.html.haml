= boxed_form_for Score::Result.model_name.human, @score_result do |f|
  = f.error_notification
  = f.inputs do
    = f.input :name
    = f.association :assessment
    = f.input :group_assessment, as: :boolean
    = render 'tag_form', f: f
  = f.actions do
    = cancel_link
    = f.primary_button :submit, "Speichern"
